<templateSet group="JavaScript">
  <template name="log" value="console.log($END$);" description="Inserts console log" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="dbg" value="debugger; // eslint-disable-line no-debugger" description="debugger" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="flow" value="// @flow strict-local&#10;" description="Inserts @flow annotation" toReformat="true" toShortenFQNames="true">
    <context />
  </template>
  <template name="if" value="if ($EXPRESSION$) {&#10;    $END$&#10;}&#10;" description="If statement - if (e) {}" toReformat="true" toShortenFQNames="true">
    <variable name="EXPRESSION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>